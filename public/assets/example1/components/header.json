{
    "type": "Page",
    "title": "头部",
    "name": "Login",
    "header": [],
    "content": [{
        "api": "${api}/def/header",
        "type": "Header",
        "title": "头部",
        "successUrl": "/",
        "env": {
            "info": "请输入${`name}${`label}.."
        },
        "prompting": {
            "success": "登录成功",
            "error": "登录失败",
            "confirm": "确定增加",
            "loading": "正在登录..."
        },
        "menu": {
            
        },
        "items": [{
                "label": "用户名",
                "name": "username",
                "required": true,
                "type": "Input",
                "defaultValue": "",
                "placeholder": "${info}"
            },
            {
                "label": "密码",
                "name": "password",
                "required": true,
                "type": "InputPassword",
                "defaultValue": "",
                "placeholder": "${info}"
            }
        ],
        "controls": [{
                "type": "CancelButton",
                "label": "返回"
            },
            {
                "type": "SubmitButton",
                "label": "登录"
            }
        ]
    }],
    "footer": []
}

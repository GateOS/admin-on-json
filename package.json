{
  "name": "admin-on-json",
  "version": "0.1.0",
  "private": true,
  "port": 8080,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "run-p tsc lessc",
    "dev.bak": "run-p tsc-w lessc-w",
    "dev": "run-p dev-server dev-client",
    "tsc": "tsc",
    "lessc": "lessc-glob  src/**/*.less lib",
    "pub": "run-s compile bundle",
    "bundle": "run-s bundle-js bundle-css",
    "bundle-css": "cp lib/index.css bundle/saul.css",
    "bundle-js": "webpack --config webpack.config.js",
    "tsc-w": "tsc -w",
    "lessc-w": "watchy -w src -- bash -c 'npm run lessc'",
    "server": "http-server -p 3301",
    "dev-server": "node dev-server.js",
    "dev-client": "webpack --config webpack.dev.js -w",
    "start": "run-p dev-server dev-client"
  },
  "dependencies": {
    "@types/node": "^8.0.13",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "history": "^4.6.3",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.0.10",
    "redux-dialog": "^0.0.9",
    "redux-logger": "^3.0.6",
    "redux-router": "^2.1.2",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.2.0",
    "reload": "^2.0.0",
    "ts-loader": "^2.3.0",
    "typescript": "^2.4.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "devDependencies": {
    "@types/react": "^15.0.8",
    "@types/react-dom": "~0.14.18",
    "awesome-typescript-loader": "^3.2.1",
    "css-split-webpack-plugin": "^0.2.3",
    "less": "^2.7.2",
    "lessc-glob": "gastrodia/lessc-glob",
    "npm-run-all": "^4.0.2",
    "watchy": "^0.6.7",
    "webpack": "1.14.0"
  }
}
